      <bpmndi:BPMNShape id="EndEvent_1_di" bpmnElement="EndEvent_Approved">
        <dc:Bounds x="692" y="99" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="672" y="142" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_2_di" bpmnElement="EndEvent_Rejected">
        <dc:Bounds x="692" y="222" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="672" y="265" width="76" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="SequenceFlow_1_di" bpmnElement="SequenceFlow_1">
        <di:waypoint x="215" y="117" />
        <di:waypoint x="270" y="117" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="SequenceFlow_2_di" bpmnElement="SequenceFlow_2">
        <di:waypoint x="370" y="117" />
